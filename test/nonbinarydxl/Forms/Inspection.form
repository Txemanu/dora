<action title='Print Goods Received Note'><imageref name='action_print.gif'/><code
 event='click'><lotusscript>Sub Click(Source As Button)
	' Call agent will full permissions to create GRN
	' then take picture and put it on blank doc for printing.
	Dim ws 		As New NotesUIWorkspace
	Dim s 		As New NotesSession
	Dim uidoc 	As NotesUIDocument
	Dim doc 		As NotesDocument
	Dim agent 	As NotesAgent
	Dim view 		As NotesView
	Dim gdoc		As NotesDocument
	Dim db		As NotesDatabase
	
	
	Set uidoc 	= ws.CurrentDocument
	Set doc		= uidoc.Document 
	Set db		= s.CurrentDatabase	
	Set view		= db.GetView("GoodsReceivedPrint")
	
	Set agent = db.GetAgent("OpenGRN")
	If agent.RunOnServer(doc.NoteID) = 0 Then	
		Set gdoc	= view.GetDocumentByKey(doc.OrderNumber(0))
		If Not(gdoc Is Nothing) Then Call ws.EditDocument(False, gdoc)		
	End If	
End Sub</lotusscript></code></action></sharedactionref>
