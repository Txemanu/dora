><text>Yes|1</text><text>No|0</text></textlist></keywords><code event='defaultvalue'><formula
>"No"</formula></code></field></run><run><font size='8pt' name='Arial' pitch='variable'
 truetype='true' familyid='20'/>  </run><actionhotspot hotspotstyle='none'><run
><font size='8pt' style='underline' name='Arial' pitch='variable' truetype='true'
 familyid='20' color='blue'/>Create Now</run><code event='options'><lotusscript
>Use "Jord.General"

</lotusscript></code><code event='click'><lotusscript>Sub Click(Source As Button)
	' get a link to the inspection report db
	' create an inspection report
	
	Dim ws 		As New notesuiworkspace
	Dim uidoc 	As notesuidocument
	Dim s 		As New notessession
	Dim doc 		As NotesDocument
	Dim currdoc 	As NotesDocument
	Dim db 		As NotesDatabase
	Dim insptype 	As String
	Dim jsstr 	As String
	Dim jsvar		As Variant
	Dim fjob		As String
	
	'..init
	Set uidoc = ws.CurrentDocument
	Set currdoc = uidoc.Document
	
	' Get the inspection reports database
	Set db = getDb(s, "inspection_reports_db")
	
	insptype = currdoc.inspectionRequired(0)
	If insptype 		= "Final" Then
		insptype 		= "Release"
	Elseif insptype 	= "Progressive" Then
		insptype 		= "Progress"
	End If
	
	Set doc = db.CreateDocument
	With doc
		.form 				= "InspRep"		
		.Date 				= Today
		.equipDesc 			= currdoc.orderDescription(0)
		.puchaseOrder 	= currdoc.orderNumber(0)
		.vendorName 		= currdoc.supplierName(0)
		.type 					= insptype
		.orderInitiator 	= currdoc.orderInitiator(0)
	End With	
	
	If Instr(currDoc.JobnoList(0), ", ")&gt;0 Then
		doc.JobNo 			= Split(currDoc.JobnoList, ", ")						
	Else
		doc.JobNo 			= currDoc.JobnoList(0)		
	End If
	
	If 	Left(doc.JobNo(0), 1) = "M" Then
		Set jdb	= getdb(s, "malaysia_job_summaries_db")
	Else
		Set jdb	= getdb(s, "job_summaries_db")		
	End If
	
	Set jview	= jdb.GetView("AllJobs")
	
	namesstr = doc.OrderInitiator(0) 
	'set the default distribution settings
	For x=0 To Ubound(doc.JobNo)		
		Set jdoc = jview.GetDocumentByKey(doc.jobno(x),True)
		If Not(jdoc Is Nothing) Then
			namesstr = namesstr + ","+jdoc.ContractEngineer(0) +","+jdoc.DesignEngineer(0)+","+jdoc.OperationsManager(0)
			PrimaryFieldEngineer = PrimaryFieldEngineer + ","+jdoc.PrimaryFieldEngineer(0)	
		End If	
	Next
	doc.defaultsendto = Split(namesstr,",")
	doc.FieldEngineer = Split(PrimaryFieldEngineer,",")
	
	Call ws.EditDocument(True, doc)
End Sub</lotusscript></code></actionhotspot><run><font size='8pt' name='Arial'
 pitch='variable' truetype='true' familyid='20'/></run></par></tablecell></tablerow>
