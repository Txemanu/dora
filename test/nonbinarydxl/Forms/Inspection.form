<globals><code event='options'><lotusscript>Option Public
Option Explicit
Use "Jord.General"
Use "OrdersScripts"


</lotusscript></code><code event='PromptTaxRate'><lotusscript>Sub PromptTaxRate
	Dim ses As New NotesSession
	Dim ws As New NotesUIWorkspace
	Dim uidoc As NotesUIDocument
	Dim dbThis As NotesDatabase
	Dim vwItems As NotesView
	Dim dcItems As NotesDocumentCollection	
	Dim doc As NotesDocument
	Dim docItem As NotesDocument
	Dim varRate As Variant
	Dim strOrderID As String
	Dim initRate As Variant
	
	'...init
	Set dbThis = ses.CurrentDatabase
	Set uidoc = ws.CurrentDocument
	Set doc = uidoc.Document
	
	'...prompt for tax rate
	If uidoc.FieldGetText("taxRate") = "" Then
		initRate = ""
	Else
		initRate = uidoc.FieldGetText("taxRate")
	End If
	
	varRate = ws.Prompt ( PROMPT_OKCANCELEDIT , "Tax Rate" , _
	"Enter the tax rate as a number only" +Chr(13)+"e.g. ten and a half percent tax = 10.5" +Chr(13)+"For no tax enter in 0" , Cstr(initRate))
	
	If Not Isempty ( varRate ) Then
		
		'...set in this doc
		doc.taxRate = varRate
		
		'...update any child item docs
		Set vwItems = dbThis.GetView( "ItemsByOrderID" )
		strOrderID = doc.OrderID( 0 )
		Set dcItems = vwItems.GetAllDocumentsByKey( strOrderID , True )
		If dcItems.Count &gt; 0 Then
			Set docItem = dcItems.GetFirstDocument
			While Not docItem Is Nothing
				Call docItem.ReplaceItemValue( "taxRate" , Cdbl(doc.taxRate( 0 ) ))
				Call docItem.ComputeWithForm( False , False )
				Call docItem.Save( True , True )
				Set docItem = dcItems.GetNextDocument( docItem )
			Wend
		End If
		
		Call doc.Save(True, True)
		uidoc.Refresh
		uidoc.Refresh
		Call uidoc.Save
	End If	
End Sub
</lotusscript></code><code event='NextLineNumber'><lotusscript>Function NextLineNumber(orderID As String) As Double
	Dim s As New NotesSession
	Dim db As NotesDatabase
	Dim view As NotesView
	Dim nav As NotesViewNavigator
	Dim entry As NotesViewEntry
	
	Set db = s.CurrentDatabase
	Set view = db.GetView("EmbedItems")
	Set nav = view.CreateViewNavFromCategory(orderID)
	If Not(nav Is Nothing) Then
		If Not(nav.Count = 0) Then
			Set entry = nav.GetLastDocument
			NextLineNumber = entry.ColumnValues(1) + 1
		Else
			NextLineNumber = 1
		End If	
	Else 
		NextLineNumber = 1
	End If
	
End Function
</lotusscript></code><code event='checkfornull'><lotusscript>Function checkfornull(newone As Variant) As Variant
	If newone = "" Then
		checkfornull = ""
	Else
		checkfornull = newone
	End If
End Function</lotusscript></code></globals><code event='windowtitle'><formula
>"Inspection"</formula></code><code event='querysave'><lotusscript>Sub Querysave(Source As Notesuidocument, Continue As Variant)
	
	Dim s 		As New NotesSession
	Dim db 		As NotesDatabase
	Dim doc 		As NotesDocument
	Dim docComment As NotesDocument
	Dim docEmail 	As NotesDocument
	Dim rtitem 	As NotesRichTextItem
	
	Set db 	= s.CurrentDatabase
	Set doc 	= Source.Document
	
	'...if we have new inspection signoff then create response and clear inspection signoff fields
	If doc.inspectionConducted( 0 ) = "1" Then
		' .. if final then mark as awiting PM to sign off
		If (doc.inspectionRequired(0) = "Final") And (doc.projManagerSignoff(0) = "-1")Then
			doc.FinalInspection = "1"			
		End If
		
		'...create inspection comment and set fields
		Set docInspect 			= db.CreateDocument
		docInspect.Form 			= "InspectionComment"
		docInspect.orderID 			= doc.orderID
		docInspect.inspectionReport 	= doc.inspectionReport
		docInspect.releaseCertificate = doc.releaseCertificate
		
		docInspect.ComputeWithForm False , False
		
		'...save comment and make it a response
		Call docInspect.MakeResponse( doc )
		docInspect.save True , True
		
		'...send off email notification if release cert issued
		If doc.releaseCertificate( 0 ) = "1" Then
			
			Set docEmail 		= db.CreateDocument
			docEmail.Form 		= "Memo"
			docEmail.SendTo	=  doc.projectManagers
			docEmail.Subject 	= "Release certificate has been issued for purchase order " + doc.orderNumber( 0 )
			Set rtitem 		= New NotesRichTextItem( docEmail , "Body" )
			Call rtitem.AppendText ( "Release certificate has been issued for purchase order " + doc.orderNumber( 0 ) + _
			Chr(13) + "Please click here to view the inspection document --&gt; " )
			Call rtitem.AppendDocLink( doc , doc.orderNumber( 0 ) )
			Call docEmail.Send( True )
			
		' if the persons add a no no  then email proj manager with comments
		Elseif (doc.releaseCertificate( 0 ) = "0") And (doc.inspectionReport( 0 ) = "0") Then
			
			Set docEmail 		= db.CreateDocument
			docEmail.Form 		= "Memo"
			docEmail.SendTo	=  doc.projectManagers
			docEmail.Subject 	= "Inspection complete for purchase order " + doc.orderNumber( 0 ) + ". No Report or release certificate created"
			Set rtitem 		= New NotesRichTextItem( docEmail , "Body" )
			Call rtitem.AppendText ( "Inspection complete for purchase order " + doc.orderNumber( 0 ) + _
			Chr(13) + "No inspection report or release certificate have been created. " + Chr(13) + _
			Chr(13)+ "Comments:" +Chr(13) + doc.inspectionComment(0) + Chr(13) +Chr(13)+ _ 
			"Click here to open the inspection document ------&gt; ")
			Call rtitem.AppendDocLink( doc , doc.orderNumber( 0 ) )
			Call docEmail.Send( True )
			
		End If
		
		'...clear the inspection signoff fields
		doc.inspectionConducted 	= ""
		doc.inspectionReport 	= "No"
		doc.releaseCertificate 	= "No"
		doc.inspectionComment 	= ""
		
		Source.Refresh
		
	Elseif (doc.projManagerSignoff(0) = "1" Or doc.projManagerSignoff(0) = "0") Then
		
		Set docEmail 		= db.CreateDocument
		docEmail.Form 		= "Memo"
		docEmail.SendTo	=  doc.inspector(0)
		Set rtitem 		= New NotesRichTextItem( docEmail , "Body" )
		
		If doc.projManagerSignoff(0) = "1" Then
			
			docEmail.Subject = "Inspection has been Signed Off for order " + doc.orderNumber( 0 ) + " as complete"
			Call rtitem.AppendText ( "Inspection has been signed off the by project manager. ")
			
		Else
			
			doc.FinalInspection 	= ""		
			docEmail.Subject 		= "Inspection has NOT been Signed Off for order " + doc.orderNumber( 0 ) + ". Please address these issues ASAP"			
			Call rtitem.AppendText ( "Inspection has NOT been signed off the by project manager. ")
			
		End If
		
		Call rtitem.AppendText(Chr(13) +Chr(13)+ "COMMENTS:" +Chr(13) + doc.EmailComments(0) + Chr(13) +Chr(13)+ _ 
		"Click here to open the inspection document ------&gt; ")
		Call rtitem.AppendDocLink( doc , doc.orderNumber( 0 ) )
		Call docEmail.Send( True )
		
	End If
	
End Sub</lotusscript></code>
