	inspdb.FilePath, _
	"ByOrderNo", _
	"Select Inspection Report to view", _
	"If you can see the 'Purchase Order No' column it means that NO inspection report has been done.", doc.OrderNumber(0) )
	
	If collection.Count &lt;&gt; 0 Then
