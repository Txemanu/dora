	'...initialise
	Set uidoc = ws.CurrentDocument
	Set doc = uidoc.Document
	If doc.currentRevUNID( 0 ) = "" Then
		Msgbox "Order Not Found", , "The order could not be found."		
	Else
		Set ordersDb = getDB( s , "orders_db" )
		Set docOrder = ordersDb.GetDocumentByUNID( doc.currentRevUNID( 0 ) )
