><text>Inspection Conducted|1</text></textlist></keywords><code event='onchange'><lotusscript
>Sub Onchange(Source As Field)
	Dim s As New NotesSession
	Dim db As NotesDatabase
	Dim ws As New notesuiworkspace
	Dim uidoc As NotesUIDocument
	Dim doc As NotesDocument
	Dim tmpdoc As NotesDocument
	
	Set uidoc = ws.CurrentDocument
	Set doc = uidoc.Document
	Set db = s.CurrentDatabase
	Set tmpDoc = db.CreateDocument
	
	If (Right(Cstr(((doc.IDFlag(0) / 2) *10)), 1)  = "0") And (doc.inspectionConducted(0) = "1") Then
		If (Cstr(doc.inspectionDate(0)) &lt;&gt; "") And (doc.inspectionRequired(0) = "Progressive")Then
			Dim dateTime As New NotesDateTime( doc.InspectionDate(0) )
			Call dateTime.AdjustDay( Cint(doc.ProgressInspectionRate(0)) )
			tmpDoc.inspectionDate = dateTime.DateOnly
		Else 
			tmpDoc.inspectionDate = Today
		End If	
		
		flag =  ws.DialogBox("NextInspectionDate",True , True , True , False , False , False , "Select Next Inspection Date" , tmpDoc , True , False , False )	
		If flag = True Then
			doc.inspectionDate = tmpDoc.inspectionDate(0)			
		End If		
		'Call uidoc.Refresh
	End If	
	doc.IDFlag = doc.IDFlag(0) + 1
End Sub</lotusscript></code></field></run><run><font size='8pt' name='Arial'
 pitch='variable' truetype='true' familyid='20'/></run></par></tablecell></tablerow>
